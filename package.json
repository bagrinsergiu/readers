{
  "name": "@brizy/readers",
  "version": "1.0.1",
  "description": "Brizy readers package",
  "keywords": [
    "brizy",
    "readers"
  ],
  "author": "brizy.io",
  "license": "MIT",
  "homepage": "https://github.com/bagrinsergiu/readers#readme",
  "bugs": "https://github.com/bagrinsergiu/readers/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bagrinsergiu/readers.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm run clean && npm run build:types && webpack --watch",
    "build": "npm run clean && webpack --mode production && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint \"src/**/!(*.d).ts*\"",
    "tsc": "tsc --noEmit",
    "prettier-check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "test": "jest",
    "check": "npm run lint && npm run tsc && npm run prettier-check && npm run test",
    "prepublishOnly": "npm run check && npm run build"
  },
  "devDependencies": {
    "@swc/core": "^1.3.73",
    "@swc/jest": "^0.2.36",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.2",
    "minimist": "^1.2.8",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "files": [
    "dist",
    "package.json"
  ]
}
