    "jest": "^29.6.2",
    "minimist": "^1.2.8",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "swc-loader": "^0.2.3",
    "typescript": "^5.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1"
